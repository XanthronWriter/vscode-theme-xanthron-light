    "tokenColors": [
        {
            "name": "Dark Red",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#810000"
            }
        },
        {
            "name": "Dark Blue",
            "scope": [
                "storage.modifier",
                "keyword.other.binding",
                "entity.name.namespace"
            ],
            "settings": {
                "foreground": "#1e2af3"
            }
        },
        {
            "name": "Dark Blue Bold",
            "scope": [
                "keyword.control",
                "keyword.function",
                "variable.language.self"
            ],
            "settings": {
                "foreground": "#1e2af3",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Light Blue",
            "scope": [
                "keyword.type",
                "meta.type_params",
                "keyword.other",
                "invalid",
                "support.type",
                "entity.name.type.enumeration",
                "meta.type-parameters",
                "entity.name.function",
                "keyword.local.lua",
                "support.function.general.tex"
            ],
            "settings": {
                "foreground": "#3f90f6"
            }
        },
        {
            "name": "Light Turquoise",
            "scope": [
                "storage.type",
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#2ebfcc"
            }
        },
        {
            "name": "Light Turquoise Bold",
            "scope": [
                "support.type.primitive",
                "entity.name.type.numeric"
            ],
            "settings": {
                "foreground": "#2ebfcc",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Dark Orange",
            "scope": [
                "keyword.operator.borrow.and",
                "storage.modifier.lifetime",
                "keyword.operator.dereference",
                "entity.name.type.core",
                "meta.attribute"
            ],
            "settings": {
                "foreground": "#fa4a18"
            }
        },
        {
            "name": "Dark Orange Bold",
            "scope": [
                "punctuation.definition.interpolation",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#fa4a18",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Light Orange",
            "scope": [
                "constant.numeric",
                "string"
            ],
            "settings": {
                "foreground": "#ed8817"
            }
        },
        {
            "name": "Light Orange Bold",
            "scope": [],
            "settings": {
                "foreground": "#ed8817",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Dark Yellow",
            "scope": [
                "_entity.name.namespace"
            ],
            "settings": {
                "foreground": "#d0a200"
            }
        },
        {
            "name": "Dark Green Italic",
            "scope": [
                "comment",
                "comment.line",
                "comment.line.double-slash",
                "comment.block",
                "comment.line.double-dash",
                "punctuation.definition.comment",
                "comment.line.percentage",
                "comment.linecomment"
            ],
            "settings": {
                "foreground": "#1f6829",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Comment Documentation",
            "scope": [
                "comment.line.documentation",
                "comment.line.double-dash.doc.lua"
            ],
            "settings": {
                "foreground": "#7e7e7e",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Debug Green",
            "scope": [],
            "settings": {
                "foreground": "#00ff00"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "bitwise": "#000000"
    },
    "todo-tree.highlights.customHighlight": {}
}
